set {
  parent {
    SOURCES
  }
  ..${SOURCES}
  in-sources {
    ..@{autark -C .. glob 'fs/*.c'}
  }
}

set {
  parent {
    PUB_HDRS
  }
  ..${PUB_HDRS}
  in-sources {
    ..@{autark -C .. glob 'fs/*.h'}
  }
}

set {
  root {
    CFLAGS
  }
  ..${CFLAGS}
  -I SS{}
}

